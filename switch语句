public class switch语句 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       char today='日';
        switch(today){
        case '一':
        case '三':
        case '五':
        	System.out.println("包子");
        break;
        case '二':
        case '四':
        case '六':
        	System.out.println("油条");
        default:
        	System.out.println("主席套餐");
        }
	}

}
